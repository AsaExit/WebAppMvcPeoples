@model Person 
@{
    ViewData["Title"] = "Person Details";
}

<h1>Person Details</h1>

<dl>
    <dt>Namn</dt>
    <dd>@Model.Name</dd>

    <dt>Phonenumber</dt>
    <dd>@Model.PhoneNumber</dd>

    <dt>City</dt>
    <dd>@Model.City</dd>
</dl>

<button name="action" value="delete">Delete</button>
<a asp-action="people">back</a>
